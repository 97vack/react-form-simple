{
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@@/*": [
        ".dumi/tmp/*"
      ],
      "react-form-simple": [
        "src"
      ],
      "react-form-simple/*": [
        "src/*",
        "*"
      ],
      "@components/*": [
        ".dumi/component/*",
        "*"
      ],
      "useHook/*": [
        ".dumi/use/*",
        "*"
      ],
      "useLocalFormat": [
        ".dumi/use/useLocalFormat.ts",
      ]
    }
  },
  "include": [
    ".dumirc.ts",
    "src/**/*",
    "global.d.ts",
    "docs/demos/**/*",
    "docs/api/apiDocs/**/*",
    "docs/types/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}